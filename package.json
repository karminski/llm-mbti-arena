{
  "name": "llm-mbti-arena",
  "version": "1.0.0",
  "description": "A CLI tool for testing LLM personality types using MBTI",
  "type": "module",
  "main": "./dist/main.js",
  "bin": {
    "llmmbtibenchmark": "./dist/main.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/main.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:local": "tsx scripts/test-local-generation.ts",
    "batch": "tsx scripts/batch-benchmark.ts",
    "generate:summary": "tsx scripts/generate-summary.ts",
    "generate:image": "tsx scripts/generate-image.ts",
    "update:readme": "tsx scripts/update-readme.ts"
  },
  "keywords": [
    "llm",
    "mbti",
    "personality",
    "benchmark",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "canvas": "^3.2.0",
    "chalk": "^5.0.0",
    "chart.js": "^4.5.0",
    "chartjs-node-canvas": "^5.0.0",
    "commander": "^11.0.0",
    "openai": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
